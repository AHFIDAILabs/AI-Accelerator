{
  "name": "ai-accelerator-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Accelerator Learning Platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "npm run build && npm start",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "ts-node src/utils/seedData.ts",
    "seed:admin": "ts-node src/utils/seedAdmin.ts",
    "db:reset": "ts-node src/utils/resetDatabase.ts"
  },
  "keywords": [
    "ai",
    "accelerator",
    "learning",
    "platform",
    "education",
    "typescript",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "groq-sdk": "^0.37.0",
    "helmet": "^7.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.6",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.10.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "xlsx": "^0.18.5"
